{
  "name": "fajardm/scraperstore",
  "type": "library",
  "description": "A scraper that copies content from online store",
  "keywords": [
    "fajardm",
    "scraperstore"
  ],
  "homepage": "https://github.com/fajardm/scraperstore",
  "license": "MIT",
  "authors": [
    {
      "name": "fajar dwi mawan",
      "email": "fajar.dwi.mawan@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.1",
    "php": "~5.5|~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      ":vendor\\:package_name\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      ":vendor\\:package_name\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
